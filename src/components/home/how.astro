---
import { t, setDefaultNamespace } from "i18next";
import { Trans } from "astro-i18next/components";

const languageItems = [
  ["php", "PHP", "programming"],
  ["laravel", "Laravel", "framework"],
  ["typescript", "Typescript", "programming"],
  ["react", "React", "framework"],
  ["graphql", "GraphQL", "programming"],
  ["illustrator", "Adobe Illustrator", "software"],
  ["jamstack", "Jamstack", "cms"],
  ["javascript", "Javascript", "programming"],
  ["mongodb", "MongoDB", "database"],
  ["mysql", "MySQL", "database"],
  ["nodejs", "NodeJS", "programming"],
  ["tailwindcss", "TailwindCSS", "framework"],
  ["woocommerce", "Woocommerce", "plugin"],
  ["wordpress", "WordPress", "cms"],
  ["strapi", "Strapi", "cms"],
];

setDefaultNamespace("home");
---

<section class="px-0 py-12 lg:py-24">
  <div
    class="bg-[conic-gradient(at_bottom_right,_var(--tw-gradient-stops))] from-stone-900 via-neutral-600 to-stone-500 rounded-none px-5 lg:px-48 py-12 lg:py-24 flex flex-col items-center"
  >
    <div class="w-full md:w-2/3 pb-8">
      <h2 class="text-2xl py-3 lg:text-3xl text-center text-slate-50">
        <Trans i18nKey="how.subheader">
          <span
            class="text-transparent bg-clip-text bg-[radial-gradient(ellipse_at_left,_var(--tw-gradient-stops))] from-orange-500 via-pink-500 to-orange-500 font-bold uppercase"
            >How</span
          > we do it
        </Trans>
      </h2>
      <p class="text-center text-lg pb-3 px-0 md:pr-10 text-slate-50">
        {t("how.content")}
      </p>
    </div>

    <aside class="w-full md:w-2/3 pt-8">
      <div class="grid grid-cols-2 md:grid-cols-3 align-middle gap-4">
        {
          languageItems.map((language) => (
            <div class="flex flex-col md:flex-row items-center md:items-start py-2">
